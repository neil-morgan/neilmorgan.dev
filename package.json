{
  "name": "neilmorgan.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"app,codegen\" \"yarn dev:app\" \"yarn codegen:watch\"",
    "check": "concurrently --names \"lint,type-check\" \"yarn lint\" \"yarn type-check\"",
    "dev:app": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc -b",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config codegen.ts -w",
    "prepare": "husky install"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "dependencies": {
    "@apollo/client": "^3.8.0-rc.2",
    "@apollo/experimental-nextjs-app-support": "^0.5.2",
    "@apollo/server": "^4.7.4",
    "@as-integrations/next": "^3.0.0",
    "@contentful/rich-text-react-renderer": "^15.17.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@stitches/react": "^1.2.8",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@vercel/analytics": "^1.1.2",
    "dotenv": "^16.4.1",
    "encoding": "^0.1.13",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.2",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-merge-refs": "^2.1.1",
    "react-syntax-highlighter": "^15.5.0",
    "server-only": "^0.0.1",
    "tslib": "^2.6.0",
    "typescript": "5.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@parcel/watcher": "^2.4.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/react-syntax-highlighter": "^15.5.7",
    "concurrently": "^8.2.2",
    "husky": "^8.0.3",
    "prettier": "^3.1.0"
  }
}
