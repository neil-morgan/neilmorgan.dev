{
  "name": "neilmorgan.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"app,codegen\" \"yarn dev:app\" \"yarn --require dotenv/config codegen:watch\"",
    "dev:app": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc -b",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config codegen.ts -w",
    "prepare": "husky install",
    "find-dead-code": "ts-prune --ignore service/index.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "dependencies": {
    "@babel/core": "^7.24.9",
    "@contentful/rich-text-react-renderer": "^15.17.0",
    "@contentful/rich-text-types": "^16.8.1",
    "@plaiceholder/next": "^3.0.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-hook/resize-observer": "^2.0.1",
    "@stitches/react": "^1.2.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "18.2.17",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.12",
    "dotenv": "^16.4.1",
    "encoding": "^0.1.13",
    "eslint": "8.57.0",
    "eslint-config-next": "14.0.3",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.0.5",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.2",
    "next": "14.2.5",
    "plaiceholder": "^3.0.0",
    "react": "18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-merge-refs": "^2.1.1",
    "react-responsive-masonry": "^2.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "server-only": "^0.0.1",
    "sharp": "0.32.6",
    "tslib": "^2.6.3",
    "typescript": "5.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.9",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@parcel/watcher": "^2.4.0",
    "@testing-library/dom": "^10.3.2",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.3.1",
    "@types/react-responsive-masonry": "^2.1.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "concurrently": "^8.2.2",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3"
  }
}
